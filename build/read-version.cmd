@ECHO OFF

SET UMBRACO_RELEASE=
SET UMBRACO_COMMENT=

:: Try to get the version and comment from Version.txt lines 2 and 3
IF EXIST Version.txt (
    FOR /F "skip=1 delims=" %%i IN (Version.txt) DO IF NOT DEFINED UMBRACO_RELEASE SET UMBRACO_RELEASE=%%i
    FOR /F "skip=2 delims=" %%i IN (Version.txt) DO IF NOT DEFINED UMBRACO_COMMENT SET UMBRACO_COMMENT=%%i
)

ECHO.
ECHO UMBRACON_RELEASE: %UMBRACO_RELEASE%
ECHO UMBRACO_COMMENT: %UMBRACO_COMMENT%

